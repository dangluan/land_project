<div class="bottom-trademark-container land-container">
	<div class="trade-slides">
	<% trades_bottom.each_slice(3) do |slice|%>
		<ul class="list-trade">
			<% slice.each do |img|%>
				<li><%= image_tag img %></li>
			<% end %>
		</ul>
		<%end%>
	</div>
</div>

<script language="JavaScript">
    $(function() {
      $('div.trade-slides').slidesjs({
        width: 570,
        height: 100,
        play: {
          active: true,
          auto: true,
          interval: 10000,
          swap: true
        },
        effect: {
        	slide: {
          speed: 900
	        }
        }
      });
    });
</script>