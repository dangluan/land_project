
	<ul id="js-news" class="js-hidden">
		<% posts = Land.get_news_land%>
		<% posts.each do |post|%>
    	<li class="news-item"><a href="#" class="dynamic-link" data-url="/clients/<%= post.id%>/show" data-method="GET" data-load-element="#stage"><%= post.title %></a></li>
    <% end %>
	</ul>

<script language="JavaScript">
	$(function () {
  	$('#js-news').ticker({ speed: 0.8, displayType: 'fade', fadeInSpeed: 600, titleText: 'Breaking news' });     
  });
</script>